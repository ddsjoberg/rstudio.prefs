df <- 
  rvest::read_html("https://docs.rstudio.com/ide/server-pro/session-user-settings.html") %>% 
  rvest::html_nodes("table") %>% 
  rvest::html_table(fill = T) %>%
  purrr::pluck(1) %>%
  dplyr::mutate(
    r_type = 
      dplyr::case_when(
        Type %in% "boolean" ~ "logical",
        Type %in% "integer" ~ "integer",
        Type %in% "number" ~ "numeric",
        startsWith(Type, "string") ~ "character"
      )
  )

df %>% dplyr::filter(is.na(r_type))
with(df, table())
